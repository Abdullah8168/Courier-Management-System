         </ImageView>
         <Text fill="WHITE" fontSmoothingType="LCD" layoutX="59.0" layoutY="274.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Manage Payments">
            <font>
               <Font name="Arial Black" size="13.0" />
            </font>
         </Text>
         <Text fill="WHITE" fontSmoothingType="LCD" layoutX="60.0" layoutY="323.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Manage Inventory">
            <font>
               <Font name="Arial Black" size="13.0" />
            </font>
         </Text>
         <Text fill="WHITE" fontSmoothingType="LCD" layoutX="67.0" layoutY="372.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Manage Users" wrappingWidth="115.0">
